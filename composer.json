{
    "name": "aminetiyal/laravel-adminlte",
    "description": "AdminLTE Laravel Integration",
    "keywords": [
        "aminetiyal",
        "laravel-adminlte"
    ],
    "homepage": "https://github.com/aminetiyal/laravel-adminlte",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Amine TIYAL",
            "email": "aminetiyal@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/view": "^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Aminetiyal\\LaravelAdminlte\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aminetiyal\\LaravelAdminlte\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aminetiyal\\LaravelAdminlte\\AdminlteServiceProvider"
            ]
        }
    }
}
